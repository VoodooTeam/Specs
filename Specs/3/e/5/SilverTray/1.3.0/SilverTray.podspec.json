{
  "name": "SilverTray",
  "version": "1.3.0",
  "license": "Apache License, Version 2.0",
  "summary": "Data chunk player",
  "description": "play encoded data using AVAudioEngine",
  "homepage": "https://github.com/nugu-developers/silvertray-ios",
  "authors": {
    "childc": "skimdcc@gmail.com"
  },
  "source": {
    "git": "https://github.com/nugu-developers/silvertray-ios.git",
    "tag": "1.3.0"
  },
  "documentation_url": "https://developers.nugu.co.kr",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=*simulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=*simulator*]": "arm64"
  },
  "platforms": {
    "ios": "12.0",
    "tvos": "13.0"
  },
  "source_files": [
    "SilverTray/Classes/**/*",
    "SilverTray/Libraries/**/*.h"
  ],
  "public_header_files": [
    "SilverTray/Classes/**/*.h",
    "SilverTray/Libraries/**/*.h"
  ],
  "ios": {
    "vendored_libraries": "SilverTray/Libraries/Opus/Binary/iOS/libopus.a"
  },
  "tvos": {
    "vendored_libraries": "SilverTray/Libraries/Opus/Binary/tvOS/libopus.a"
  },
  "watchos": {
    "vendored_libraries": "SilverTray/Libraries/Opus/Binary/watchOS/libopus.a"
  },
  "osx": {
    "vendored_libraries": "SilverTray/Libraries/Opus/Binary/macOS/libopus.a"
  },
  "preserve_paths": "SilverTray/Libraries/**",
  "libraries": "c++",
  "xcconfig": {
    "OTHER_LDFLAGS": "-Xlinker -w"
  }
}
